{
  "javascript.preferences.autoImportFileExcludePatterns": ["@radix-ui"],
  "typescript.preferences.autoImportFileExcludePatterns": ["@radix-ui"],
  "IDX.aI.enableInlineCompletion": true,
  "IDX.aI.enableCodebaseIndexing": true,
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "i18n-ally.localesPaths": ["i18n", "messages"],
  "i18n-ally.keystyle": "nested",
  "i18n-ally.extract.ignoredByFiles": {
    "components\\cards\\grade-overview.tsx": ["3 5", "10 4"],
    "components\\ui\\asterisk.tsx": ["*"],
    "components\\header.tsx": ["Logo"],
    "components\\loadingscreen.tsx": [
      "\r\n            Grade\r\n            ",
      "\r\n            Calculator\r\n          "
    ]
  },
  "i18n-ally.extract.ignored": [
    "${truncatedGrade} ${\r\n            hideText\r\n              ? \"\"\r\n              : t(\"grades.grades\", { count: average.average?.gradeAmount })\r\n          }"
  ]
}
